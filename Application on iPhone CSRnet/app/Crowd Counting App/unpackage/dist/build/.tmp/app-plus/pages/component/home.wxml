<view><scroll-view class="page" scroll-y><image class="response" src="{{png}}" mode="widthFix"></image><view class="nav-list"><block wx:for="{{elements}}" wx:for-item="item" wx:for-index="index" wx:key="index"><navigator class="{{['nav-li','bg-'+item.color]}}" style="{{'width:100%;'+('animation:'+('show '+((index+1)*0.2+1)+'s 1')+';')}}" data-url="{{item.url}}" id="{{item.id}}" hover-class="none" navigateTo data-event-opts="{{[['tap',[['NavChange',['$event']]]]]}}" bindtap="__e"><view class="nav-title">{{item.title}}</view><view class="nav-name">{{item.name}}</view><text class="{{['cuIcon-'+item.cuIcon]}}"></text></navigator></block></view><view class="cu-tabbar-height"></view><block wx:if="{{loadModal}}"><view class="cu-load load-modal"><view class="gray-text">服务器识别中...</view></view></block></scroll-view></view>